   "execution_count": 10,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/latex": [
       "$\\displaystyle x \\left(- 3 h + x\\right) \\left(- 2 h + x\\right) \\left(- h + x\\right)$"
      ],
